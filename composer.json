{
    "name": "tivins/framework",
    "description": "PHP Framework for web applications",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Tivins\\Framework\\": "src/",
            "Tivins\\Framework\\Tests\\": "tests/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": ">=8.1",
        "erusev/parsedown": "^1.7",
        "tivins/database":  "dev-main",
        "tivins/php-common": "dev-main",
        "ext-readline": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "bin": [
        "bin/install.php"
    ]
}
